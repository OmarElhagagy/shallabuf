{
	"[javascript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescript]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[json]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"[yaml]": {
		"editor.defaultFormatter": "biomejs.biome"
	},
	"editor.codeActionsOnSave": {
		"quickfix.biome": "explicit",
		"source.organizeImports.biome": "explicit"
	},
	"editor.formatOnSave": true,
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.preferences.autoImportFileExcludePatterns": [
		"next/router.d.ts",
		"next/dist/client/router.d.ts"
	],
	"terminal.integrated.localEchoStyle": "dim",
	"search.exclude": {
		"**/node_modules": true
	},
	"cSpell.words": [
		"addgroup",
		"adduser",
		"alexpass",
		"appgroup",
		"appuser",
		"bindgen",
		"biomejs",
		"bobopass",
		"bobpass",
		"bunx",
		"chrono",
		"clsx",
		"cust",
		"dall",
		"devel",
		"dotenv",
		"dotenvy",
		"dtos",
		"EXAT",
		"hdel",
		"healthcheck",
		"hgetall",
		"hset",
		"Instantiator",
		"jarredsumner",
		"jetstream",
		"lockb",
		"lucide",
		"lukemathwalker",
		"nextjs",
		"nuqs",
		"objcopy",
		"OPENPANEL",
		"petgraph",
		"pino",
		"plpgsql",
		"Ratelimit",
		"repr",
		"reqwest",
		"sadd",
		"serde",
		"Shadcn",
		"shallabuf",
		"smembers",
		"sqlx",
		"srem",
		"subcomponents",
		"SUPABASE",
		"tailwindcss",
		"topo",
		"tsbuildinfo",
		"turbopack",
		"Turborepo",
		"typecheck",
		"UPSTASH",
		"wasi",
		"wasmtime",
		"xyflow"
	],
	"rust-analyzer.inlayHints.lifetimeElisionHints.useParameterNames": true,
	"rust-analyzer.inlayHints.closureReturnTypeHints.enable": "always",
	"rust-analyzer.inlayHints.reborrowHints.enable": "never",
	"rust-analyzer.semanticHighlighting.punctuation.enable": true,
	"rust-analyzer.diagnostics.styleLints.enable": true,
	"rust-analyzer.check.extraArgs": [
		"--",
		"-W",
		"clippy::needless-pass-by-value",
		"-W",
		"clippy::pedantic",
		"-A",
		"clippy::module-name-repetitions",
		"-A",
		"clippy::cast-lossless",
		"-A",
		"clippy::wildcard-imports",
		"-A",
		"clippy::struct-field-names",
		"-A",
		"clippy::no-effect-underscore-bindings",
		"-A",
		"clippy::missing-docs-in-private-items",
		"-A",
		"clippy::missing-docs-in-private-impls",
		"-A",
		"clippy::missing-docs-in-private-trait-impls",
		"-A",
		"clippy::missing-docs-in-private-extern-crate",
		"-A",
		"clippy::missing-docs-in-private-use",
		"-A",
		"clippy::missing-errors-doc",
		"-A",
		"clippy::similar-names",
		"-A",
		"clippy::cast-possible-truncation",
		"-A",
		"clippy::cast-sign-loss",
		"-A",
		"clippy::cast-possible-loss"
	],
	"editor.fontLigatures": true
}
